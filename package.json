{
  "name": "react-web-share",
  "author": "harshzalavadiya",
  "version": "3.1.0",
  "description": "Tiny Web Share API Wrapper with fallback for unsupported browsers",
  "license": "MIT",
  "repository": "https://github.com/hc-oss/react-web-share",
  "module": "./dist/esm/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --inject-style --legacy-output --minify --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --inject-style --legacy-output --format esm,cjs --watch --dts --external react",
    "lint": "eslint src --fix",
    "size": "size-limit"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18 || ^19",
    "react-dom": "^16 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "9.36.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "size-limit": "^11.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "files": [
    "dist/**"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "react",
    "share",
    "web-share",
    "native",
    "fallback",
    "polyfill",
    "component",
    "tiny",
    "lightweight"
  ]
}
